<?xml version='1.0' encoding='utf-8'?>
<widget id="ucles.weblab.triangle.app" version="1.0.0" ios-CFBundleVersion="1.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Triangle App</name>
    <description>
        Triangle App dev version.
    </description>
    <author email="hudson.p@cambridgeassessment.org.uk" href="http://www.cambridgeassessment.org.uk/">
        Peter Hudson
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
